<!DOCTYPE html>
<html lang="{{ grav.language.getActive ?: grav.config.site.default_lang }}">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% include 'partials/metadata.html.twig' %}
    <title>{% if page.title %}{{ page.title|e('html') }} | {% endif %}{{ site.title|e('html') }}</title>
    {% do assets.addLink('theme://img/favicon.png', { rel: 'icon', type: 'image/png' }) %}
    {% block stylesheets %}

    <link rel="preload" href="{{ url('theme://fonty/fira-sans-v16-latin_latin-ext-regular.woff2') }}" as="font" crossorigin>
    <link rel="preload" href="{{ url('theme://fonty/fira-sans-extra-condensed-v10-latin_latin-ext-600.woff2') }}" as="font" crossorigin>
    <link rel="preload" href="{{ url('theme://fonty/fira-sans-v16-latin_latin-ext-700.woff2') }}" as="font" crossorigin>
    <link rel="preload" href="{{ url('theme://css/style.min.css') }}" as="style">
    {% do assets.addCss('theme://css/style.min.css') %}
    {% endblock %}

    {% block javascripts %}
    {% do assets.addJs('theme://js/popper.min.js', {group:'bottom'}) %}
    {% do assets.addJs('theme://js/bootstrap.bundle.min.js', {group:'bottom'}) %}
    {% do assets.addJs('theme://js/script.js', {group:'bottom'}) %}
    {% endblock %}


    {% block assets deferred %}
    {{ assets.css()|raw }}
    {{ assets.js()|raw }}
    {% endblock %}
</head>

<body>
    <a class="skip-to-content-link" href="#hlavni-obsah">
        Skoƒçit na obsah
      </a>

      <header class="header mb-5">
        {% include 'partials/header.html.twig' %}
    </header>

<div class="container breadcrumbs mb-5 d-md-block d-none">
<div class="row">
<div class="col-lg-7 col-md-10 col-12 mx-auto">
{% include 'partials/breadcrumbs.html.twig' %}
</div>
</div>

</div>
    <main id="hlavni-obsah" class="obsah">
        {% block content %}
        {{ page.content|raw }}
        {% endblock %}
    </main>


    <footer class="footer py-5 bg-secondary mt-5">
        <div class="container">

            {% block footer %} 
          {% include "partials/footer.html.twig" with {"page": page.find("/footer", true)} %}
            {% endblock %}
           
        </div>
       
    </footer>

